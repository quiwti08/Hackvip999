RunService.Heartbeat:Connect(function()
    if resolverEnabled then
        for _,plr in ipairs(Players:GetPlayers()) do
            if plr ~= Players.LocalPlayer and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                -- Reset velocity để aim không bị lệch
                plr.Character.HumanoidRootPart.Velocity = Vector3.new(0, plr.Character.HumanoidRootPart.Velocity.Y, 0)
            end
        end
    end
end)

-- AutoBring Toggle (Da Hood chuẩn)
local autoBringEnabled = false
local autoBringConn

local autoBringButton = CreateButton(ButtonFrame1, "AutoBring: OFF", function()
    autoBringEnabled = not autoBringEnabled
    autoBringButton.Text = "AutoBring: " .. (autoBringEnabled and "ON" or "OFF")
    sendNotification("AutoBring", autoBringEnabled and "Enabled" or "Disabled")

    if autoBringEnabled then
        autoBringConn = RunService.Heartbeat:Connect(function()
            local myChar = LocalPlayer.Character
            local myHRP = myChar and myChar:FindFirstChild("HumanoidRootPart")
            local hum = myChar and myChar:FindFirstChildOfClass("Humanoid")
            if not (myHRP and hum and hum.Health > 0) then return end

            for name, selected in pairs(Config.SelectedPlayers or {}) do
                if selected then
                    local plr = Players:FindFirstChild(name)
                    local char = plr and plr.Character
                    local hrp = char and char:FindFirstChild("HumanoidRootPart")
                    local bodyEffects = char and char:FindFirstChild("BodyEffects")
                    local ko = bodyEffects and bodyEffects:FindFirstChild("K.O")

                    if hrp and ko and ko.Value == true then
                        -- Bring target về trước mặt
                        hrp.CFrame = myHRP.CFrame * CFrame.new(0,0,-3)

                        -- Nếu chưa bị Grab → gửi remote Grab
                        local grabVal = bodyEffects and bodyEffects:FindFirstChild("Grabbed")
                        if grabVal and grabVal.Value == false then
                            local mainEvent = game:GetService("ReplicatedStorage"):FindFirstChild("MainEvent")
                            if mainEvent then
                                mainEvent:FireServer("Grabbing", plr)
                            end
                        end
                    end
                end
            end
        end)
    else
        if autoBringConn then
            autoBringConn:Disconnect()
            autoBringConn = nil
        end
    end
end)

-- AutoStomp Selected Toggle
local autoStompEnabled = false
local autoStompConn

local autoStompButton = CreateButton(ButtonFrame1, "AutoStomp: OFF", function()
    autoStompEnabled = not autoStompEnabled
    autoStompButton.Text = "AutoStomp: " .. (autoStompEnabled and "ON" or "OFF")
    sendNotification("AutoStomp", autoStompEnabled and "Enabled" or "Disabled")

    if autoStompEnabled then
        autoStompConn = RunService.Heartbeat:Connect(function()
            local myChar = LocalPlayer.Character
            local myHRP = myChar and myChar:FindFirstChild("HumanoidRootPart")
            local hum = myChar and myChar:FindFirstChildOfClass("Humanoid")
            if not (myHRP and hum and hum.Health > 0) then return end

            for name, selected in pairs(Config.SelectedPlayers or {}) do
                if selected then
                    local plr = Players:FindFirstChild(name)
                    local char = plr and plr.Character
                    local lowerTorso = char and char:FindFirstChild("LowerTorso")
                    local bodyEffects = char and char:FindFirstChild("BodyEffects")
                    local ko = bodyEffects and bodyEffects:FindFirstChild("K.O")

                    if lowerTorso and ko and ko.Value == true then
                        -- Lưu vị trí cũ
                        local oldCFrame = myHRP.CFrame

                        -- Đến gần vị trí LowerTorso của victim
                        myHRP.CFrame = lowerTorso.CFrame * CFrame.new(0, 0, 3)

                        -- Stomp
                        local mainEvent = game:GetService("ReplicatedStorage"):FindFirstChild("MainEvent")
                        if mainEvent then
                            mainEvent:FireServer("Stomp")
                        end

                        -- Delay nhỏ để chắc chắn stomp xong rồi về lại
                        task.delay(0.5, function()
                            if myHRP and autoStompEnabled then
                                myHRP.CFrame = oldCFrame
                            end
                        end)
                    end
                end
            end
        end)
    else
        if autoStompConn then
            autoStompConn:Disconnect()
            autoStompConn = nil
        end
    end
end)

-- 0Delay Toggle (tích hợp AutoGunPatch)
local zeroDelayEnabled = false
local zeroDelayConn

local zeroDelayButton = CreateButton(ButtonFrame1, "0Delay: OFF", function()
    zeroDelayEnabled = not zeroDelayEnabled
    zeroDelayButton.Text = "0Delay: " .. (zeroDelayEnabled and "ON" or "OFF")
    sendNotification("0Delay", zeroDelayEnabled and "Enabled" or "Disabled")

    -- Hàm patch súng
    local function patchGun(tool)
        if not tool or not zeroDelayEnabled then return end

        -- Patch FireRate / ShotDelay / ReloadTime
        local fireRate = tool:FindFirstChild("FireRate") or tool:FindFirstChild("ShotDelay") or tool:FindFirstChild("ReloadTime")
        if fireRate and fireRate:IsA("NumberValue") then
            fireRate.Value = 0
        end

        -- Ammo vô hạn
        local ammo = tool:FindFirstChild("Ammo") or tool:FindFirstChild("AmmoCount")
        if ammo and ammo:IsA("IntValue") then
            ammo.Value = math.huge
        end
    end

    -- Xử lý khi bật
    if zeroDelayEnabled then
        -- Patch tool đang cầm
        local char = LocalPlayer.Character
        if char then
            for _, tool in ipairs(char:GetChildren()) do
                if tool:IsA("Tool") then
                    patchGun(tool)
                end
            end
        end

        -- Theo dõi equip mới
        local function hookChar(c)
            c.ChildAdded:Connect(function(child)
                if child:IsA("Tool") then
                    task.wait(0.1)
                    patchGun(child)
                end
            end)
        end

        if LocalPlayer.Character then
            hookChar(LocalPlayer.Character)
        end
        LocalPlayer.CharacterAdded:Connect(hookChar)
    else
        -- Tắt thì thôi, không restore (vì thường game sẽ tự reset value khi respawn)
    end
end)

local function isAlive(plr)
    if not plr or not plr.Character then return false end
    local hum = plr.Character:FindFirstChildOfClass("Humanoid")
    if not hum or hum.Health <= 0 then
        return false
    end

    -- Check riêng cho Da Hood (BodyEffects)
    local be = plr.Character:FindFirstChild("BodyEffects")
    if be then
        local ko = be:FindFirstChild("K.O")
        local grabbed = be:FindFirstChild("Grabbed")
        if (ko and ko.Value) or (grabbed and grabbed.Value) then
            return false
        end
    end

    return true
end

local function getClosestSelected()
    local myChar = LocalPlayer.Character
    local myHRP = myChar and myChar:FindFirstChild("HumanoidRootPart")
    if not myHRP then return nil end
    local closest, minDist
    for name, selected in pairs(Config.SelectedPlayers or {}) do
        if selected then
            local plr = Players:FindFirstChild(name)
            local hrp = plr and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart")
            if hrp then
                local dist = (myHRP.Position - hrp.Position).Magnitude
                if not minDist or dist < minDist then
                    closest, minDist = plr, dist
                end
            end
        end
    end
    return closest
end

local function getNextTarget(currentTarget)
    local myChar = LocalPlayer.Character
    local myHRP = myChar and myChar:FindFirstChild("HumanoidRootPart")
    if not myHRP then return nil end
    local candidates = {}
    for name, selected in pairs(Config.SelectedPlayers or {}) do
        if selected then
            local plr = Players:FindFirstChild(name)
            if plr and plr ~= currentTarget and plr.Character and plr.Character:FindFirstChild("HumanoidRootPart") then
                table.insert(candidates, plr)
            end
        end
    end
    table.sort(candidates, function(a, b)
        local ha = a.Character and a.Character:FindFirstChild("HumanoidRootPart")
        local hb = b.Character and b.Character:FindFirstChild("HumanoidRootPart")
        if not ha or not hb then return false end
        return (myHRP.Position - ha.Position).Magnitude < (myHRP.Position - hb.Position).Magnitude
    end)
    return candidates[1]
end

